<!DOCTYPE HTML><html><head><cfif NOT structKeyExists(url,'p')>	<cflocation url="index.cfm"></cfif><cfinclude template="inc_includes_all.cfm"><meta charset="utf-8"><cfset page_keyword = replacenocase(url.p,'_',' ','all')><cfset qrs_p = get_pages_keyword(dsn,page_keyword)><cfif NOT qrs_p.recordcount>	<cflocation url="index.cfm"></cfif>          <meta name="description" content="<cfoutput>#qrs_p.meta_description#</cfoutput>"><meta name="keywords" content="<cfoutput>#qrs_p.meta_keywords#</cfoutput>"><title><cfoutput>#qrs_p.tag_title#</cfoutput></title><cfset scriptCssLinks()></head><body><!-- header --><cfset headerNav(dsn,qrs_p.nav_main_id)><!-- /header --><div class="container" id="single">       <div>&nbsp;    </div>      <section id="page">    <div class="row">        <div class="span3">      <h2>Customer Service</h2>      <h3>Our #1 Priority</h3>      <hr/>      <div style="text-align:center">      <a style="margin-left:auto;margin-right:auto;" class="btn btn-large btn-primary" target="quote" href="<cfoutput>#QUOTE_URL#</cfoutput>">FREE QUOTE <i class="icon-comment icon-white"></i></a>      </div>             <hr/>       <h2>Our Partners</h2>       <cfset show_partners(dsn)>    </div>    <div class="span9">            <cfset subNav(false,dsn)><hr/>      	  <cfoutput>      <h1>#qrs_p.headline#</h1>      <div style="margin-top:10px;">      <cfif len(qrs_p.img)>      	<img src="assets/img_pages/#suffixAppend(qrs_p.img,'_300')#" style="float:right;margin:0 0 5px 15px;border:1px solid ##ddd;">      </cfif>      #qrs_p.body#</div>	  	  </cfoutput>    </div>    </div>  </section>    <hr class="space"></div><!-- end container --><cfset footer_code()>   <script type="text/javascript"> 	  var _gaq = _gaq || [];	  _gaq.push(['_setAccount', 'UA-???-1']);	  _gaq.push(['_trackPageview']);	 	  (function() {		var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;		ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';		var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);	  })();	 	</script></body></html>