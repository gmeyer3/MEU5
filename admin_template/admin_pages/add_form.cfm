<div class="accordion-group">  <div class="accordion-heading"> <a href="#collapseQuickV2" data-parent="#accordion2" data-toggle="collapse" class="accordion-toggle btn btn-primary" style="text-align: left;text-decoration: none"> <i class="icon-edit"></i> Add Item <i class="icon-plus pull-right"></i> </a> </div>  <div class="accordion-body collapse" id="collapseQuickV2"> <!--- <cfif NOT structKeyExists(url,'edit')> in</cfif> --->    <div style="background-color: #ffffff;" class="accordion-inner">      <h2>        <cfif opt EQ 1>          Pages: Main Navbar                    <cfelseif opt EQ 2>          Pages: Sub Navbar                    <cfelseif opt EQ 3>          Pages: Splash Tab                  </cfif>        Items Admin</h2>      <hr>      <form action="<cfoutput>#thisScript#?#formArgs#</cfoutput>" method="post" enctype="multipart/form-data" class="form-horizontal" id="Add_form">        <div class="control-group">          <label class="control-label" for="inputEmail">Key Word</label>          <div class="controls">            <input type="text" name="keyword" class="text" maxlength="100" />          </div>        </div>        <cfif opt EQ 1>          <div class="control-group">            <label class="control-label" for="inputEmail">Main Navigation ID</label>            <div class="controls">              <cfset qrs_nav_main_sections = get_nav_main_sections(dsn)>              <select name="nav_main_id">                                <!---  --->                 <cfoutput query="qrs_nav_main_sections">                  <option value="#ID#">#title#</option>                </cfoutput>              </select>            </div>          </div>        </cfif>        <cfif opt EQ 2>          <div class="control-group">            <label class="control-label" for="inputEmail">Sub Navigation ID</label>            <div class="controls">              <cfset qrs_nav_sub_sections = get_nav_sub_sections(dsn)>              <select name="nav_sub_id">                                <!---  --->                 <cfoutput query="qrs_nav_sub_sections">                  <option value="#ID#">#title#</option>                </cfoutput>              </select>            </div>          </div>        </cfif>        <cfif opt EQ 3>          <div class="control-group">            <label class="control-label" for="inputEmail">Tab Sections ID</label>            <div class="controls">              <cfset qrs_tab_sections = get_tab_sections(dsn)>              <select name="tab_sections_id">                                <!---  --->                 <cfoutput query="qrs_tab_sections">                  <option value="#ID#">#title#</option>                </cfoutput>              </select>            </div>          </div>        </cfif>        <cfif structKeyExists(url,'nav_main')>          <div class="control-group">            <label class="control-label" for="inputEmail">Main Nav Priority</label>            <div class="controls">              <input type="text" name="nav_main_priority" class="input-mini"/>            </div>          </div>        </cfif>        <cfif structKeyExists(url,'nav_sub')>          <div class="control-group">            <label class="control-label" for="inputEmail">Sub Nav Priority</label>            <div class="controls">              <input type="text" name="nav_sub_priority" class="input-mini"/>            </div>          </div>        </cfif>        <cfif structKeyExists(url,'tab')>          <div class="control-group">            <label class="control-label" for="inputEmail">Tab Sections Priority</label>            <div class="controls">              <input type="text" name="tab_sections_priority" class="input-mini"/>            </div>          </div>        </cfif>        <div class="control-group">          <label class="control-label" for="inputEmail">Photo</label>          <div class="controls">            <input type="file" name="img" class="text" maxlength="100" />          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">Photo ALT tag in splash page</label>          <div class="controls">            <input type="text" name="img_alt_splash" class="text" maxlength="100" />          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">Photo ALT tag in content page</label>          <div class="controls">            <input type="text" name="img_alt_page" class="text" maxlength="100" />          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">meta_description</label>          <div class="controls">            <input type="text" name="meta_description" class="text" maxlength="100" />          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">meta_keywords</label>          <div class="controls">            <input type="text" name="meta_keywords" class="text" maxlength="100" />          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">Page TITLE tag</label>          <div class="controls">            <input type="text" name="tag_title" class="text" maxlength="100" />          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">Headline</label>          <div class="controls">            <input type="text" name="headline" class="text" maxlength="100" />          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">Sub Headline for Tab Display</label>          <div class="controls">            <input type="text" name="sub_headline" class="text" maxlength="100" />          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">Body Content</label>          <div class="controls">            <textarea name="body" class="textarea span12" rows="10"></textarea>          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">Enabled?</label>          <div class="controls"> YES            <input type="radio" name="enabled" value="1" checked="checked"/>            NO            <input type="radio" name="enabled" value="0" />          </div>        </div>                <!--- <div class="control-group">                                <label class="control-label" for="inputEmail">priority</label>                                <div class="controls">                                                                                            <input type="text" name="priority" class="input-mini"/>                                                                                                </div>                            </div> --->                <div class="control-group">          <label class="control-label" for="inputEmail">google_conversion_code</label>          <div class="controls">                      <textarea name="google_conversion_code" class="span12" rows="10"></textarea>          </div>        </div>        <button class="btn btn-primary btn-large"><span>ADD </span> <i class="icon-plus icon-white"></i></button>        <input type="hidden" name="action" value="add" />      </form>      <div id="submit_progress_add" class="progress progress-striped progress-primary active" style="display:none;">        <div class="bar" style="width: 80%;"></div>      </div>    </div>  </div></div><!-- end accordion  -->