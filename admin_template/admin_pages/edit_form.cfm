<cfset qrs_pages = get_pages(dsn,url.edit)><cfoutput>  <div class="accordion-group">    <div class="accordion-heading"> <a href="##collapseQuickV3" data-parent="##accordion2" data-toggle="collapse" class="accordion-toggle btn btn-warning" style="text-align: left;text-decoration: none"> <i class="icon-edit"></i> Edit Item <i class="icon-edit pull-right"></i> </a> </div>    <div class="accordion-body collapse in" id="collapseQuickV3">      <div style="background-color: ##ffffff;" class="accordion-inner">        <h2>          <cfif opt EQ 1>            Pages: Main Navbar            <cfset formArgs = 'nav_main=true'>            <cfelseif opt EQ 2>            Pages: Sub Navbar            <cfset formArgs = 'nav_sub=true'>            <cfelseif opt EQ 3>            Pages: Splash Tab            <cfset formArgs = 'tab=true'>          </cfif>          Items Admin</h2>        <hr>        <form action="<cfoutput>#thisScript#?#formArgs#</cfoutput>" method="post" enctype="multipart/form-data" class="form-horizontal" id="Edit_form">          <div class="control-group">            <label class="control-label" for="inputEmail">Key Word</label>            <div class="controls">              <input type="text" name="keyword" value="#qrs_pages.keyword#" class="text" maxlength="100" />            </div>          </div>          <cfif opt EQ 1>            <div class="control-group">              <label class="control-label" for="inputEmail">Main Navigation ID</label>              <div class="controls">                <cfset qrs_nav_main_sections = get_nav_main_sections(dsn)>                <cfset qrs_nav_main_selected = get_nav_main_sections(dsn,#qrs_pages.nav_main_id#)>                <select name="nav_main_id">                  <option value="#qrs_nav_main_selected.id#" selected="selected">#qrs_nav_main_selected.title#</option>                  <cfloop query="qrs_nav_main_sections">                    <option value="#ID#">#qrs_nav_main_sections.title#</option>                  </cfloop>                </select>              </div>            </div>            <cfelse>            <input type="hidden" name="nav_main_id" value="">          </cfif>          <cfif opt EQ 2>            <div class="control-group">              <label class="control-label" for="inputEmail">Sub Navigation ID</label>              <div class="controls">               <!---  <cfset qrs_nav_sub_sections = get_nav_sub_sections(dsn)> --->                <cfset qrs_nav_sub_selected = get_nav_sub_sections(dsn,#qrs_pages.nav_sub_id#)>                <!--- <select name="nav_sub_id">                  <option value="#qrs_nav_sub_selected.id#" selected="selected">#qrs_nav_sub_selected.title#</option>                  <cfloop query="qrs_nav_sub_sections">                    <option value="#ID#">#qrs_nav_sub_sections.title#</option>                  </cfloop>                </select> --->                #qrs_nav_sub_selected.title#                <input type="hidden" name="nav_sub_id" value="#qrs_nav_sub_selected.id#">              </div>            </div>            <cfelse>            <input type="hidden" name="nav_sub_id" value="">          </cfif>          <cfif opt EQ 3>            <div class="control-group">              <label class="control-label" for="inputEmail">Tab Sections ID</label>              <div class="controls">                <cfset qrs_tab_sections = get_tab_sections(dsn)>                <cfset qrs_tab_sections_selected = get_tab_sections(dsn,#qrs_pages.tab_sections_id#)>                <select name="tab_sections_id">                  <option value="#qrs_tab_sections_selected.id#" selected="selected">#qrs_tab_sections_selected.title#</option>                  <cfloop query="qrs_tab_sections">                    <option value="#ID#">#qrs_tab_sections.title#</option>                  </cfloop>                </select>              </div>            </div>            <cfelse>            <input type="hidden" name="tab_sections_id" value="">          </cfif>          <cfif opt EQ 1>            <div class="control-group">              <label class="control-label" for="inputEmail">Main Nav Priority</label>              <div class="controls">                <input type="text" name="nav_main_priority" class="input-mini"  value="#qrs_pages.nav_main_priority#"/>              </div>            </div>            <cfelse>            <input type="hidden" name="nav_main_priority" value="">          </cfif>          <cfif opt EQ 2>            <!--- <div class="control-group">              <label class="control-label" for="inputEmail">Sub Nav Priority</label>              <div class="controls">                <input type="text" name="nav_sub_priority" class="input-mini"  value="#qrs_pages.nav_sub_priority#"/>              </div>            </div> --->            <input type="hidden" name="nav_sub_priority" value="#qrs_pages.nav_sub_priority#">            <cfelse>            <input type="hidden" name="nav_sub_priority" value="">          </cfif>          <cfif opt EQ 3>            <div class="control-group">              <label class="control-label" for="inputEmail">Tab Sections Priority</label>              <div class="controls">                <input type="text" name="tab_sections_priority" class="input-mini"  value="#qrs_pages.tab_sections_priority#"/>              </div>            </div>            <cfelse>            <input type="hidden" name="tab_sections_priority" value="">          </cfif>          <div class="control-group">            <label class="control-label" for="inputEmail"> <a href="javascript:void(0);" onclick="imgModal('../assets/img_pages/#suffixAppend(qrs_pages.img,"_800")#');" > <img src="../assets/img_pages/#suffixAppend(qrs_pages.img,'_60')#"> </a> Photo</label>            <div class="controls">              <input type="file" name="img" placeholder="#qrs_pages.img#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Photo ALT tag in splash page</label>            <div class="controls">              <input type="text" name="img_alt_splash" value="#qrs_pages.img_alt_splash#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Photo ALT tag in content page</label>            <div class="controls">              <input type="text" name="img_alt_page" value="#qrs_pages.img_alt_page#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">meta_description</label>            <div class="controls">              <input type="text" name="meta_description" value="#qrs_pages.meta_description#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">meta_keywords</label>            <div class="controls">              <input type="text" name="meta_keywords" value="#qrs_pages.meta_keywords#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Page TITLE tag</label>            <div class="controls">              <input type="text" name="tag_title" value="#qrs_pages.tag_title#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Headline</label>            <div class="controls">              <input type="text" name="headline" value="#qrs_pages.headline#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Sub Headline for Tab Display</label>            <div class="controls">              <input type="text" name="sub_headline" value="#qrs_pages.sub_headline#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Body Content</label>            <div class="controls">              <textarea name="body" class="textarea span12" rows="10">#qrs_pages.body#</textarea>            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Enabled?</label>            <div class="controls"> YES              <input type="radio" name="enabled" value="1" <cfif qrs_pages.enabled eq 1>checked="checked"</cfif> />              NO              <input type="radio" name="enabled" value="0" <cfif qrs_pages.enabled eq 0>checked="checked"</cfif> />            </div>          </div>                    <!--- <div class="control-group">                                <label class="control-label" for="inputEmail">priority</label>                                <div class="controls">                                                                                            <input type="text" name="priority" class="input-mini"  value="#priority#"/>                                                                                                </div>                            </div> --->                    <div class="control-group">            <label class="control-label" for="inputEmail">google_conversion_code</label>            <div class="controls">              <textarea name="google_conversion_code" class="span12" rows="10">#qrs_pages.google_conversion_code#</textarea>            </div>          </div>          <button class="btn btn-warning btn-large"><span>SAVE </span> <i class="icon-edit icon-white"></i></button>          <button type="reset" class="btn btn-large"><span>RESET </span> <i class="icon-ban-circle icon-white"></i></button>          <input type="hidden" name="action" value="edit" />          <input type="hidden" name="rid" value="#qrs_pages.ID#" />        </form>        <div id="submit_progress_edit" class="progress progress-striped progress-warning active" style="display:none;">          <div class="bar" style="width: 80%;"></div>        </div>      </div>    </div>  </div>  <!-- end accordion  --> </cfoutput>