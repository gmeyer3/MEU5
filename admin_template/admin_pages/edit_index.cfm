<div class="accordion" id="accordionPagination">  <div class="accordion-group">    <div class="accordion-heading"> <a style="text-align: left;text-decoration: none" class="accordion-toggle btn"                           data-toggle="collapse" data-parent="#accordion2" href="#collapsePagination"> <i class="icon-list"></i> Pages <i class="icon-minus pull-right"></i> </a> </div>    <div id="collapsePagination" class="accordion-body collapse in">      <div class="accordion-inner" style="background-color: #ffffff;">        <cfset qrs_pages_options = get_pages_options2(dsn,opt)>        <table cellpadding="0" cellspacing="0" border="0" class="table table-bordered" id="example">          <thead>            <tr>              <th></th>              <th>Keyword</th>              <cfif opt NEQ 2><th>Priority</th></cfif>              <th>Category</th>              <th>Image</th>             <cfif opt NEQ 2> <th></th></cfif>            </tr>          </thead>          <tbody>            <cfoutput query="qrs_pages_options">              <tr<cfif NOT enabled> class="not_enabled"</cfif>>                <td><a href="#thisScript#?#formArgs#&edit=#id#" class="btn btn-mini btn-warning"><i class="icon-edit"></i> edit</a></td>                <td>#keyword#</td>                <td class="center"><cfif structKeyExists(url,'nav_main')>                    #nav_main_priority#                    <cfelseif structKeyExists(url,'tab')>                    #tab_sections_priority#                  </cfif></td>                <td><cfif structKeyExists(url,'nav_main')>                    #nav_main_sections_title#                    <cfelseif structKeyExists(url,'nav_sub')>                    #nav_sub_sections_title#                    <cfelseif structKeyExists(url,'tab')>                    #tab_sections_title#                  </cfif></td>                <td><cfif qrs_pages_options.img NEQ ''>                    <button style="cursor:pointer" onclick="imgModal('../assets/img_pages/#suffixAppend(qrs_pages_options.img,"_600")#');" class="btn btn-mini"><i class="icon-picture"></i> zoom</button>                  </cfif></td>                <cfif opt NEQ 2> <td class="center"><button class="btn btn-mini btn-danger" onclick="confirmation(#id#,'#keyword#');"><i class="icon-trash"></i> delete</button></td> </cfif>              </tr>            </cfoutput>          </tbody>        </table>      </div>    </div>  </div></div>