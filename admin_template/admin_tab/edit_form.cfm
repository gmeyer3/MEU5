<cfset qrs_tab_sections = get_tab_sections(dsn,url.edit)><cfoutput>  <div class="accordion-group">    <div class="accordion-heading"> <a href="##collapseQuickV3" data-parent="##accordion2" data-toggle="collapse" class="accordion-toggle btn btn-warning" style="text-align: left;text-decoration: none"> <i class="icon-edit"></i> Edit Item <i class="icon-edit pull-right"></i> </a> </div>    <div class="accordion-body collapse in" id="collapseQuickV3">      <div style="background-color: ##ffffff;" class="accordion-inner">        <h2>Splash Tab Items Admin</h2>        <hr>        <form action="#thisScript#" method="post" enctype="multipart/form-data" class="form-horizontal" id="Edit_form">          <div class="control-group">            <label class="control-label" for="inputEmail">Title</label>            <div class="controls">              <input type="text" name="title" value="#qrs_tab_sections.title#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Enabled?</label>            <div class="controls"> YES              <input type="radio" name="enabled" value="1" <cfif qrs_tab_sections.enabled EQ 1>checked="checked"</cfif> />              NO              <input type="radio" name="enabled" value="0" <cfif qrs_tab_sections.enabled EQ 0>checked="checked"</cfif> />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Priority</label>            <div class="controls">              <input type="text" name="priority" class="input-mini"  value="#qrs_tab_sections.priority#"/>            </div>          </div>          <button class="btn btn-warning btn-large"><span>SAVE </span> <i class="icon-edit icon-white"></i></button>          <button type="reset" class="btn btn-large"><span>RESET </span> <i class="icon-ban-circle icon-white"></i></button>          <input type="hidden" name="action" value="edit" />          <input type="hidden" name="rid" value="#url.edit#"/>        </form>      </div>    </div>  </div>  <!-- end accordion  --> </cfoutput>