<cfsilent><cfset fail = "0"><cfinclude template="inc_includes_all.cfm"><cftry><!--- url argument processing ---><cfif structKeyExists(url,'a')><!--- NOTE: url argument 'a' refers to action. --->	<cfif url.a EQ 'nav_accord_company_open'>  <!--- 'nav_accord_company_open' --->    	<cfset session.nav_accord_company_open = true>    </cfif>    <cfif url.a EQ 'nav_accord_company_close'><!--- 'nav_accord_company_close' --->    	<cfset session.nav_accord_company_open = false>    </cfif>                <cfif url.a EQ 'nav_accord_partner_open'>  <!--- 'nav_accord_partner_open' --->    	<cfset session.nav_accord_partner_open = true>    </cfif>    <cfif url.a EQ 'nav_accord_partner_close'><!--- 'nav_accord_partner_close' --->    	<cfset session.nav_accord_partner_open = false>    </cfif></cfif><cfcatch type="any"><cfset fail = "1"></cfcatch></cftry></cfsilent><cfoutput>#fail#</cfoutput>