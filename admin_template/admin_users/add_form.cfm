<div class="accordion-group">  <div class="accordion-heading"> <a href="#collapseQuickV2" data-parent="#accordion2" data-toggle="collapse" class="accordion-toggle btn btn-primary" style="text-align: left;text-decoration: none"> <i class="icon-edit"></i> Add Item <i class="icon-plus pull-right"></i> </a> </div>  <div class="accordion-body collapse" id="collapseQuickV2"><!--- <cfif NOT structKeyExists(url,'edit')> in</cfif> --->    <div style="background-color: #ffffff;" class="accordion-inner">      <h2>Users Admin</h2>      <hr>      <form action="<cfoutput>#thisScript#</cfoutput>" method="post" enctype="multipart/form-data" class="form-horizontal" id="Add_form">        <div class="control-group">          <label class="control-label" for="inputEmail">Username</label>          <div class="controls">            <input type="text" name="username" class="text" maxlength="8" />          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">Password</label>          <div class="controls">            <input type="text" name="password" class="text" maxlength="50" />          </div>        </div>        <div class="control-group">          <cfset qrs_user_auth_level = get_user_auth_level(dsn)>          <label class="control-label" for="inputEmail">Auth Level</label>          <div class="controls">            <select name="user_auth_level_id">              <cfoutput query="qrs_user_auth_level">                <cfif session.userlevel LTE qrs_user_auth_level.ID>                  <option value="#qrs_user_auth_level.ID#">#qrs_user_auth_level.title#</option>                </cfif>              </cfoutput>            </select>          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">Email</label>          <div class="controls">            <input type="text" name="email" class="text" maxlength="100" />          </div>        </div>        <div class="control-group">          <label class="control-label" for="inputEmail">Enabled?</label>          <div class="controls"> YES            <input type="radio" name="enabled" value="1" checked="checked" />            NO            <input type="radio" name="enabled" value="0" />          </div>        </div>        <button class="btn btn-primary btn-large"><span>ADD </span> <i class="icon-plus icon-white"></i></button>        <input type="hidden" name="action" value="add" />      </form>    </div>  </div></div><!-- end accordion  -->