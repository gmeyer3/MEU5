<cfset qrs_users = get_admin_users(dsn,url.edit)><cfoutput>  <div class="accordion-group">    <div class="accordion-heading"> <a href="##collapseQuickV3" data-parent="##accordion2" data-toggle="collapse" class="accordion-toggle btn btn-warning" style="text-align: left;text-decoration: none"> <i class="icon-edit"></i> Edit Item <i class="icon-edit pull-right"></i> </a> </div>    <div class="accordion-body collapse in" id="collapseQuickV3">      <div style="background-color: ##ffffff;" class="accordion-inner">        <h2>Users Admin</h2>        <hr>        <form action="#thisScript#" method="post" enctype="multipart/form-data" class="form-horizontal" id="Edit_form">          <div class="control-group">            <label class="control-label" for="inputEmail">Username</label>            <div class="controls">              <input type="text" name="username" value="#qrs_users.username#" class="text" maxlength="8" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Password <a href="javascript:void(0);" class="btn btn-mini btn-warning" onClick="allow_pw_reset();" id="Edit_form_pw_reset_btn"><i class="icon-edit" id="Edit_form_pw_reset_btn"></i> reset</a> <a href="javascript:void(0);" style="display:none;" class="btn btn-mini btn-danger" onClick="disallow_pw_reset();" id="Edit_form_pw_cancel_btn"><i class="icon-edit"></i> cancel</a> </label>            <div class="controls">              <input type="password" id="Edit_form_pw" name="password" class="text" maxlength="50" disabled="disabled" />            </div>          </div>          <div class="control-group">            <cfset qrs_user_auth_level = get_user_auth_level(dsn)>            <cfset qrs_user_auth_level_selected = get_user_auth_level(dsn,qrs_users.user_auth_level_id)>            <label class="control-label" for="inputEmail">Auth Level</label>            <div class="controls">              <select name="user_auth_level_id">                <cfloop query="qrs_user_auth_level">                  <option value="#qrs_user_auth_level.ID#" <cfif qrs_user_auth_level.ID EQ qrs_user_auth_level_selected.id>selected="selected"</cfif>>#qrs_user_auth_level.title#</option>                </cfloop>              </select>            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Email</label>            <div class="controls">              <input type="text" name="email" value="#qrs_users.email#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Enabled?</label>            <div class="controls"> YES              <input type="radio" name="enabled" value="1" <cfif qrs_users.enabled EQ 1>checked="checked"</cfif> />              NO              <input type="radio" name="enabled" value="0" <cfif qrs_users.enabled EQ 0>checked="checked"</cfif> />            </div>          </div>          <button class="btn btn-warning btn-large"><span>SAVE </span> <i class="icon-edit icon-white"></i></button>          <button type="reset" class="btn btn-large"><span>RESET </span> <i class="icon-ban-circle icon-white"></i></button>          <input type="hidden" name="action" value="edit" />          <input type="hidden" name="rid" value="#url.edit#"/>        </form>      </div>    </div>  </div>  <!-- end accordion  --> </cfoutput>