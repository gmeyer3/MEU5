<div class="accordion" id="accordionPagination">  <div class="accordion-group">    <div class="accordion-heading"> <a style="text-align: left;text-decoration: none" class="accordion-toggle btn"                           data-toggle="collapse" data-parent="#accordion2" href="#collapsePagination"> <i class="icon-list"></i> Company Locations <i class="icon-minus pull-right"></i> </a> </div>    <div id="collapsePagination" class="accordion-body collapse in">      <div class="accordion-inner" style="background-color: #ffffff;">        <cfset qrs_company_locations = get_company_locations(#dsn#)>        <table cellpadding="0" cellspacing="0" border="0" class="table table-bordered" id="example">          <thead>            <tr>              <th></th>              <th>Address</th>              <th>City</th>              <th>Priority</th>              <th></th>            </tr>          </thead>          <tbody>            <cfoutput query="qrs_company_locations">              <tr<cfif NOT enabled> class="not_enabled"</cfif>>                <td><a href="#thisScript#?edit=#id#" class="btn btn-mini btn-warning"><i class="icon-edit"></i> edit</a></td>                <td>#address_1#</td>                <td>#city#</td>                <td class="center">#Priority#</td>                <td class="center"><button class="btn btn-mini btn-danger" onClick="confirmation(#id#,'#address_1#');"><i class="icon-trash"></i> delete</button></td>              </tr>            </cfoutput>          </tbody>        </table>      </div>    </div>  </div></div>