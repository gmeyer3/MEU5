<cfset qrs_company_locations = get_company_locations(dsn,url.edit)><cfoutput>  <div class="accordion-group">    <div class="accordion-heading"> <a href="##collapseQuickV3" data-parent="##accordion2" data-toggle="collapse" class="accordion-toggle btn btn-warning" style="text-align: left;text-decoration: none"> <i class="icon-edit"></i> Edit Item <i class="icon-edit pull-right"></i> </a> </div>    <div class="accordion-body collapse in" id="collapseQuickV3">      <div style="background-color: ##ffffff;" class="accordion-inner">        <h2>Company Locations Admin</h2>        <hr/>        <form action="<cfoutput>#thisScript#</cfoutput>" method="post" enctype="multipart/form-data" class="form-horizontal" id="Edit_form">          <div class="control-group">            <label class="control-label" for="inputEmail">Address 1</label>            <div class="controls">              <input type="text" name="address_1" value="#qrs_company_locations.address_1#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Address 2</label>            <div class="controls">              <input type="text" name="address_2" value="#qrs_company_locations.address_2#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">City</label>            <div class="controls">              <input type="text" name="city" value="#qrs_company_locations.city#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">State</label>            <div class="controls">              <input type="text" name="state" value="#qrs_company_locations.state#" class="text" maxlength="2" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Postal</label>            <div class="controls">              <input type="text" name="postal" value="#qrs_company_locations.postal#" class="text" maxlength="11" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Phone 1</label>            <div class="controls">              <input type="text" name="phone_1" value="#qrs_company_locations.phone_1#" class="text" maxlength="20" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Phone 2</label>            <div class="controls">              <input type="text" name="phone_2" value="#qrs_company_locations.phone_2#" class="text" maxlength="20" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Fax</label>            <div class="controls">              <input type="text" name="fax" value="#qrs_company_locations.fax#" class="text" maxlength="20" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Email 1</label>            <div class="controls">              <input type="text" name="email_1" value="#qrs_company_locations.email_1#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Email 2</label>            <div class="controls">              <input type="text" name="email_2" value="#qrs_company_locations.email_2#" class="text" maxlength="100" />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">enabled?</label>            <div class="controls"> YES              <input type="radio" name="enabled" value="1" <cfif qrs_company_locations.enabled eq 1>checked="checked"</cfif> />              NO              <input type="radio" name="enabled" value="0" <cfif qrs_company_locations.enabled eq 0>checked="checked"</cfif> />            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Priority</label>            <div class="controls">              <input type="text" name="priority" class="input-mini"  value="#qrs_company_locations.priority#"/>            </div>          </div>          <div class="control-group">            <label class="control-label" for="inputEmail">Primary Location?</label>            <div class="controls"> YES              <input type="radio" name="is_primary_location" value="1" <cfif qrs_company_locations.is_primary_location eq 1>checked="checked"</cfif> />              NO              <input type="radio" name="is_primary_location" value="0" <cfif qrs_company_locations.is_primary_location eq 0>checked="checked"</cfif> />            </div>          </div>          <button class="btn btn-warning btn-large"><span>SAVE </span> <i class="icon-edit icon-white"></i></button>          <button type="reset" class="btn btn-large"><span>RESET </span> <i class="icon-ban-circle icon-white"></i></button>          <input type="hidden" name="action" value="edit" />          <input type="hidden" name="rid" value="#qrs_company_locations.id#" />        </form>      </div>    </div>  </div>  <!-- end accordion  --> </cfoutput>