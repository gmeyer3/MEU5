<cffunction name="socLinks" output="yes"><cfargument name="dsn" required="yes" type="string"><cfset qrs = get_company(arguments.dsn)> <div style="margin-top:-37px; width:550px; height:40px;float:right;text-align:right;">        <cfif len(qrs.url_twitter)>    	<a href="#qrs.url_twitter#" target="twitter"><img src="img/icon_twitter_32.png" width="32" height="32" alt="Twitter" title="Twitter"></a>    </cfif>    <cfif len(qrs.url_linkedin)>    	<a href="#qrs.url_linkedin#" target="linkedin"><img src="img/icon_linkedin_32.png" width="32" height="32" alt="LinkedIn" title="LinkedIn"></a>    </cfif>    <cfif len(qrs.url_facebook)>    	<a href="#qrs.url_facebook#" target="facebook"><img src="img/icon_facebook_32.png" width="32" height="32" alt="Facebook" title="Facebook"></a>    </cfif>    <cfif len(qrs.url_blog)>    	<a href="#qrs.url_blog#" target="blog"><img src="img/icon_blog_32.png" width="32" height="32" alt="Blog" title="Blog"></a>    </cfif><br />    <a class="btn btn-primary" href="mailto:#qrs.main_email#">#qrs.main_email#</a></div></cffunction><cffunction name="scriptCssLinks" output="yes"><script type="text/javascript" src="js/jquery-1.8.0.min.js"></script><script type="text/javascript" src="js/bootstrap-dropdown.js"></script><script type="text/javascript" src="js/bootstrap-tab.js"></script>    <link href="css/bootstrap.min.css" rel="stylesheet" type="text/css">    <link href="css/render.css" rel="stylesheet" type="text/css">        <link rel="shortcut icon" href="img/favicon.ico"> 		<!-- Le HTML5 shim, for IE6-8 support of HTML elements -->		<!--[if lt IE 9]>		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>    <![endif]--></cffunction><cffunction name="headerNav" output="yes"><cfargument name="dsn" required="yes" type="string"><cfargument name="active" default="0"><cfif arguments.active EQ ''>	<cfset arguments.active = 0></cfif><header class="header">  <div class="container">    <div class="row">      <div class="span2"> <a href="index.cfm"><img id="page_logo" src="assets/logo_4.png" alt="Teachers Insurance Specialists" width="200"></a> </div>      <div class="span10">               <cfset socLinks(arguments.dsn)>                        <cfset qrs_get_main_sections_pages = get_main_sections_pages(arguments.dsn)>                <ul class="nav nav-pills header-nav" style="float:right;">          <li<cfif NOT arguments.active> class="active"</cfif>><a href="index.cfm">Home</a></li>          <cfoutput query="qrs_get_main_sections_pages" group="ID">		   <li class="dropdown<cfif arguments.active EQ ID> active</cfif>"><a class="dropdown-toggle" data-toggle="dropdown" href="##">#title#<b class="caret"></b></a>          	<cfif len(keyword)>            <ul class="dropdown-menu">            	<cfoutput>				<li><a href="page.cfm?p=#keyW(keyword)#">#keyword#</a></li>				</cfoutput>            </ul>            </cfif>          </li>		  </cfoutput>                 </ul>                      </div>    </div>  </div></header></cffunction><cffunction name="subNav" output="yes"><cfargument name="showEmail" default="true">	<cfargument name="dsn" required="yes" type="string">	<cfset qrs_get_sub_sections_pages = get_sub_sections_pages(arguments.dsn)>    <cfset qrs_company =  get_company(arguments.dsn)>    <!-- <div class="btn-group"> -->    <cfoutput query="qrs_get_sub_sections_pages">    <a class="btn btn-primary" href="page.cfm?p=#keyW(keyword)#"><i class="icon-info-sign icon-white"></i> #nav_sub_sections_title#</a>    </cfoutput>        <!---  <button class="btn btn-primary"><i class="icon-white icon-eye-open"></i> About Us</button>    <button class="btn btn-primary"><i class="icon-white icon-user"></i> Contact Us</button> --->        <cfif arguments.showEmail>     <a class="btn btn-primary" href="mailto:#qrs_company.main_email#"><i class="icon-envelope icon-white"></i> Email</a>    </cfif>       <!-- </div> --></cffunction><cffunction name="footer_code" output="yes"><cfargument name="dsn" required="yes" type="string"><cfset qrs_company =  get_company(arguments.dsn)><cfset qrs_company_location = get_company_main_location(arguments.dsn)><footer class="footer">  <div class="container footer_content">    <div class="row">      <div class="span2 frow_white">        <p>&copy; 2012 <cfif year(now()) GT 2012>- #year(now())# </cfif> <strong>#qrs_company.company_name#</strong> </p>             </div>      <div class="span7 footer_render" style="text-align:center;">     <!---  <h6>Teachers Insurance Specialists Partner Companies</h6>        <img src="assets/logo_collection.gif" width="516" height="202" alt="#qrs_company.company_name# Partner Companies">         --->      </div>      <div class="span3 footer_render">        <p> <cfif len(qrs_company_location.phone_1)><strong>Phone: </strong>#qrs_company_location.phone_1# <br></cfif>          <cfif len(qrs_company_location.fax)>strong>Fax: </strong><br></cfif>          <br>          <cfif len(qrs_company_location.address_1)>          <strong>Address: </strong>#qrs_company_location.address_1# #qrs_company_location.address_2#<br>		  </cfif>          <cfif len(qrs_company_location.city)>          #qrs_company_location.city# #qrs_company_location.state# #qrs_company_location.postal#<br>		  </cfif>      </div>    </div>  </div>  <!-- Contacts or something else -->  <div class="footer_bottom">    <div class="container">      <div class="row">        <div class="span6 footer_render">          <h3>#qrs_company_location.phone_1#</h3>          <p>Call or <a class="btn btn-mini" href="mailto:#qrs_company.main_email#"><i class="icon-envelope"></i> Email Us</a></p>        </div>        <div class="span6 frow">          <div class="input-prepend">            <h3>Stay connected</h3>            <p>Keep up with us! Helpful tips and seasonal info.</p>            <cfset emailSubscription(qrs_company.main_email,qrs_company.company_name)>          </div>        </div>      </div>    </div>    <!-- /Contacts or something else -->   </div></footer></cffunction><cffunction name="tabsDisp" output="yes"><cfargument name="dsn" required="yes" type="string"><cfargument name="limit" required="no" default="0"><cfif arguments.limit EQ 0><cfset arguments.limit = 1000000></cfif>   	    <cfset qrs_tab_sections_pages = get_tab_sections_pages(arguments.dsn)>        <cfset tab_sections_item_ct = 1>                <div class="tabbable span12"> <!-- Only required for left/right tabs -->                        <ul class="nav nav-tabs">		<cfoutput query="qrs_tab_sections_pages" group="tab_sections_ID">        <li<cfif currentrow EQ 1> class="active"</cfif>><a href="##tab#tab_sections_ID#" data-toggle="tab"><i class="icon-calendar"></i> #tab_sections_title#</a></li>       </cfoutput>		        </ul>                <div class="tab-content">           <cfoutput query="qrs_tab_sections_pages" group="tab_sections_ID">                      <div class="tab-pane<cfif currentrow EQ 1> active"</cfif>" id="tab#tab_sections_ID#">                                                  <ul class="thumbnails">						 <cfset tab_sections_item_ct = 1><!--- reset before each inner loop --->						 <cfoutput>                            <cfif tab_sections_item_ct LTE arguments.limit>                                                        <li class="span3">                            <div class="thumbnail">                            <a href="page.cfm?p=#keyW(pages_keyword)#">                            <div class="thumbDiv" style="background-image:url(assets/img_pages/#suffixAppend(pages_img,'_300')#);">                           <!---  <img src="assets/img_pages/#suffixAppend(pages_img,"_med")#" alt="#pages_keyword#"> --->                            </div>                            </a>                            <h5>#pages_headline#</h5>                            <p>#pages_sub_headline#</p>                            <a class="btn" href="page.cfm?p=#keyW(pages_keyword)#">more &raquo;</a>                                                        </div>                            </li>                            </cfif>							<cfset tab_sections_item_ct += 1>                        </cfoutput>                        </ul> 						            </div>        	 </cfoutput>                </div>               </div>   </cffunction><cffunction name="show_partners" output="yes"><cfargument name="dsn" required="yes" type="string"><cfset var qrs = get_xref_partners_cats_all(arguments.dsn)><style type="text/css">.border_top_bot { border-top:1px solid ##ddd;border-bottom:1px solid ##ddd; }.border_top { border-top:1px solid ##ddd; }</style><cfoutput query="qrs" group="cat_title"><hr style="margin:0;"/><h3>#cat_title#</h3><cfoutput>	<hr style="margin:0;"/>    <div style="margin:0;padding:0;">				<cfif len(img)>            <cfif len(url)>                <a href="#url#" target="partner"><img src="assets/img_partner_logos/#suffixAppend(img,"_100")#"/></a>            <cfelse>                <img src="assets/img_partner_logos/#suffixAppend(img,"_100")#"/>            </cfif>        <cfelse>        	<cfif len(url)>                <a href="#url#" target="partner">#title#</a>            <cfelse>                #title#            </cfif>        </cfif>     </div></cfoutput></cfoutput></cffunction><cffunction name="emailSubscription" output="yes"><cfargument name="recipient" required="yes"><cfargument name="company" required="yes"><cfargument name="subscriptionType" default="base" required="no"><cfif arguments.subscriptionType EQ "base">	<cfif structKeyExists(form,'emailSubscriptionBase')>		<!--- PROCESS FORM --->        <span style="color:green">THANK YOU FOR YOUR SUBMISSION!</span>        <cfmail to="#arguments.recipient#" from="#arguments.recipient#" subject="Email Subscription request for #arguments.company#">        #form.email_address#        </cfmail>	<cfelse>        <form id="emailSubscriptionBase" class="form-inline" action="##" method="post">          <span class="add-on"><i class="icon-envelope"></i></span>          <input type="text" class="input-small span2" placeholder="Email" name="email_address" id="email_address">          <input type="hidden" name="emailSubscriptionBase" value="1"><br />          <button type="submit" class="btn" style="margin-top:7px;"><i class="icon-check"></i>Subscribe</button>        </form>        <script src="admin_template/resource/validate/jquery.validate.min.js" type="text/javascript"></script>        <script type="text/javascript" charset="utf-8">            $(document).ready(function(){                                    $('##emailSubscriptionBase').validate({                    rules: {                       email_address: {                        required: true                        ,email: true                      }                     },                    highlight: function(label) {                        $(label).closest('.control-group').addClass('error');                    },                    success: function(label) {                        label                            .text('OK!').addClass('valid')                            .closest('.control-group').addClass('success');                    },                    focusCleanup: true                  });                              }); // end document.ready        </script>    	</cfif>            </cfif></cffunction>